{
	"name": "Lórum Ipse",
	"description": "Feltölti a beviteli mezőt Lórum ipse tartalommal ami a vakszövegnek egy magyaros változata.",
	"version": "1.0",
	"background": {
		"scripts": [
			"generate-lorum-ipse-text.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*",
				"file://*/*"
			],
			"js": [
				"lorum-ipse-content-script.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "lorum-ipse.png",
		"name": "Call Lórum Ipse"
	},
	"icons": {
		"16": "lorum-ipse-16.png",
		"48": "lorum-ipse-48.png",
		"128": "lorum-ipse-128.png"
	},
	"permissions": [
		"contextMenus"
	],
	"manifest_version": 2
}
